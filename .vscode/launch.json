{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Uvicorn",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${command:python.interpreterPath}",
      "module": "inboard.start",
      "env": {
        "APP_MODULE": "inboard.app.main_fastapi:app",
        "BASIC_AUTH_USERNAME": "test_username",
        "BASIC_AUTH_PASSWORD": "plunge-germane-tribal-pillar",
        "LOG_FORMAT": "uvicorn",
        "LOG_LEVEL": "debug",
        "PORT": "1025",
        "PROCESS_MANAGER": "uvicorn",
        "RELOAD_DIRS": "inboard",
        "WITH_RELOAD": "true"
      },
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "justMyCode": true
    }
  ]
}
